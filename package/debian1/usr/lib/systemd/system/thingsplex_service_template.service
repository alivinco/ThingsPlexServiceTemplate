[Unit]
Description=IoT Flow logic engine

[Service]
Type=simple
ExecStart=/opt/thingsplex/thingsplex_service_template/thingsplex_service_template -c config.json
User=thingsplex_service_template
WorkingDirectory=/opt/thingsplex/thingsplex_service_template
Restart=always
StandardOutput=null
StandardError=null

[Install]
WantedBy=multi-user.target